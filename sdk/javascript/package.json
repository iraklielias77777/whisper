{
  "name": "@userwhisperer/sdk-js",
  "version": "1.0.0",
  "description": "JavaScript SDK for User Whisperer platform",
  "main": "lib/userwhisperer.js",
  "module": "lib/userwhisperer.esm.js",
  "types": "lib/types.d.ts",
  "files": [
    "lib/",
    "src/",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "webpack --config webpack.config.js --mode=production",
    "build:esm": "webpack --config webpack.esm.config.js --mode=production",
    "build:types": "copy src\\types.d.ts lib\\types.d.ts",
    "dev": "webpack --config webpack.config.js --mode=development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/ --ext .js,.ts",
    "lint:fix": "eslint src/ --ext .js,.ts --fix",
    "docs": "typedoc src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "analytics",
    "user-engagement",
    "personalization",
    "machine-learning",
    "customer-retention"
  ],
  "author": "User Whisperer Team",
  "license": "MIT",
  "homepage": "https://userwhisperer.ai",
  "repository": {
    "type": "git",
    "url": "https://github.com/userwhisperer/sdk-javascript.git"
  },
  "bugs": {
    "url": "https://github.com/userwhisperer/sdk-javascript/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {},
  "peerDependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
