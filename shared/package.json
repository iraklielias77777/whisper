{
  "name": "@userwhisperer/shared",
  "version": "1.0.0",
  "description": "Shared utilities and configurations for User Whisperer platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "setup:python": "pip install -r requirements.txt",
    "format": "prettier --write \"src/**/*.{ts,js,json}\""
  },
  "keywords": [
    "user-whisperer",
    "shared",
    "utilities",
    "database",
    "stream-processing"
  ],
  "author": "User Whisperer Team",
  "license": "MIT",
  "dependencies": {
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "@google-cloud/pubsub": "^4.0.7"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1"
  },
  "files": [
    "dist",
    "requirements.txt"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
} 